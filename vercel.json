{
    "version": "2.0.0",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/$1"
        }
    ]

}